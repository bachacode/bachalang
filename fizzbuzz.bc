# Fizzbuzz

function mod(num,div) {
    let number = num
    while (number >= 0) {
        let number = number - div
    }

    let number = number + div

    return number;
}

for i = 1 to 100 {
    if (mod(i,3) == 0 && mod(i,5) == 0) {
        print("fizzbuzz");
    } elseif (mod(i,3) == 0) {
        print("fizz");
    } elseif (mod(i,5) == 0) {
        print("buzz");
    } else {
        print(i);
    }
}